				</field>
				<field name="showcategory" type="list" default="1" label="MOD_LATESTBLOGS_SHOW_CATEGORY" description="MOD_LATESTBLOGS_SHOW_CATEGORY_DESC">
					<option value="0">MOD_LATESTBLOGS_NO</option>
