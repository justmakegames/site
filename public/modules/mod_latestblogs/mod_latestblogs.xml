					<option value="1">Main content</option>
				</field>
				<field name="textcount" type="text" default="0" label="MOD_LATESTBLOGS_TOTAL_CHARACTERS" description="MOD_LATESTBLOGS_TOTAL_CHARACTERS_DESC" />
